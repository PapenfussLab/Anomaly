{
  "sources": "vep, mutalyzer, myvariant",
  "aliases": {
    "vcf": "Anomaly.vcf",
    "Mutalyzer-transcripts": "ClinGenAR.transcriptAlleles[*].hgvs[*].set()",
    "Mutalyzer-gene": "ClinGenAR.transcriptAlleles[*].geneSymbol.set()",
    "Mutalyzer-hgvsC-old": "hgvsGVariant",
    "Mutalyzer-hgvsC": "refSeq",
    "Mutalyzer-hgvsP": "ClinGenAR.transcriptAlleles[0].proteinEffect.hgvs",
    "Mutalyzer-lrg": "ClinGenAR.transcriptAlleles[0].hgvs[1]"
  },
  "schemaName": "pathos_schema",
  "defaultValue": "",
  "schema": {
    "myVariant" : "vcf.variant",
    "variant": "vcf.variantCall.info.HGVSg",
    "gene": "Vep.besttx.gene_symbol",
    "consequence": "{Vep.besttx.consequence_terms}",
    "hgvsc": "Mutalyzer.hgvsC",
    "geneSymbol": "Mutalyzer.gene",
    "lrg": "Mutalyzer.lrg",
    "hgvsg": "vcf.variantCall.info.HGVSg",
    "hgvspAa1": "Mutalyzer.hgvsP",
    "vepHgvsc": "Vep.besttx.hgvsc",
    "vepHgvsp": "Vep.besttx.hgvsp",
    "readDepth": "vcf.variantCall.samples[0].DP",
    "varDepth": "vcf.variantCall.samples[0].AD",
    "varFreq": "vcf.variantCall.samples[0].FREQ",
    "fwdReadDepth": "vcf.variantCall.samples[0].RDF",
    "fwdVarDepth": "vcf.variantCall.samples[0].ADF",
    "revReadDepth": "vcf.variantCall.samples[0].RDR",
    "revVarDepth": "vcf.variantCall.samples[0].ADR",
    "chr": "vcf.variantCall.chromosome",
    "pos": "vcf.variantCall.position.start",
    "exon": "ex{MyVariant.cadd.exon}",
    "cosmic": "Vep.vepResponse[0].colocated_variants.[?(@.id =~ /COSM(\\d+)/i)].id",
    "dbsnp": "Vep.vepResponse[0].colocated_variants[?(@.id =~ /rs(\\d+)/i)].id",
    "ens_transcript": "MyVariant.cadd.gene.feature_id",
    "ens_gene": "Vep.besttx.gene_symbol",
    "ens_protein": "Vep.besttx.hgvsp",
    "existing_variation": "{Vep.vepResponse[0].colocated_variant_ids}",
    "clin_sig": "Vep.vepResponse[0].colocated_variants.*.clin_sig",
    "pubmed": "Vep.vepResponse[0].colocated_variants.*.pubmed",
    "cadd": "MyVariant.cadd.rawscore",
    "cadd_phred": "MyVariant.cadd.phred",
    "exac": "MyVariant.dbnsfp.exac.af",
    "gmaf": "MyVariant.cadd.1000g.af",
    "esp": "MyVariant.evs.ma_fin_percent.all",
    "metaLrVal": "MyVariant.dbnsfp.metalr.pred",
    "siftVal": "MyVariant.dbnsfp.sift.pred",
    "lrtVal": "MyVariant.dbnsfp.lrt.pred",
    "mutTasteVal": "MyVariant.dbnsfp.mutationtaster.pred",
    "mutAssessVal": "MyVariant.dbnsfp.mutationassessor.pred",
    "fathmmVal": "MyVariant.dbnsfp.fathmm.pred",
    "metaSvmVal": "MyVariant.dbnsfp.metasvm.pred",
    "polyphenVal": "MyVariant.dbnsfp.polyphen2.hdiv.pred",
    "numamps": "vcf.info.numAmps",
    "amps": "vcf.info.amps",
    "ampbias": "vcf.info.ampbias",
    "homopolymer": "vcf.info.homopolymer",
    "varcaller": "vcf.info.Identified",
    "ens_variant": "Vep.vepResponse[0].id"
  }
}